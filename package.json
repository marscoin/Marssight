{
  "name": "marssight",
  "version": "1.0.0",
  "description": "Marssight - Modern Marscoin Blockchain Explorer",
  "private": true,
  "scripts": {
    "install:all": "cd frontend && npm install && cd ../api && npm install",
    "start:api": "cd api && INSIGHT_NETWORK=livenet node insight.js",
    "start:frontend": "cd frontend && npm run dev",
    "start": "npm run start:api & npm run start:frontend",
    "build": "cd frontend && npm run build",
    "test:api": "cd api && npm run test:pure"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/marscoin/Marssight.git"
  },
  "keywords": [
    "marscoin",
    "blockchain",
    "explorer",
    "cryptocurrency",
    "mars"
  ],
  "author": "Marscoin Foundation",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  }
}
